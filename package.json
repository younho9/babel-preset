{
	"name": "@younho9/babel-preset",
	"version": "0.4.0",
	"description": "Sharable set of Babel plugins and config options",
	"keywords": [
		"babel",
		"babel-preset"
	],
	"bugs": {
		"url": "https://github.com/younho9/babel-preset/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/younho9/babel-preset.git"
	},
	"license": "MIT",
	"author": {
		"name": "Younho Choo",
		"email": "yo+github@younho9.com",
		"url": "https://younho9.dev"
	},
	"type": "commonjs",
	"exports": "./dist/index.js",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rimraf dist && babel src -d dist --source-maps",
		"prepare": "npm run build"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"dependencies": {
		"@babel/helper-plugin-utils": "^7.14.5",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"babel-plugin-add-module-exports": "1.0.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.4",
		"@babel/core": "^7.15.4",
		"@types/babel__helper-plugin-utils": "7.10.0",
		"rimraf": "3.0.2"
	},
	"peerDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
